<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/window.xsd"
        caption="msg://editorCaption"
        class="com.artwellhk.alertsystem.web.alertsnooze.AlertSnooze2Edit"
        datasource="alertSnoozeDs"
        focusComponent="fieldGroup"
        messagesPack="com.artwellhk.alertsystem.web.alertsnooze">
    <dsContext>
        <datasource id="alertSnoozeDs"
                    class="com.artwellhk.alertsystem.entity.AlertSnooze"
                    view="_local"/>
    </dsContext>
    <dialogMode height="600"
                width="800"/>
    <layout expand="windowActions"
            spacing="true">
        <fieldGroup id="fieldGroup"
                    datasource="alertSnoozeDs">
            <column width="250px">
                <field property="sampleOrderId"/>
                <field property="duration"/>
            </column>
        </fieldGroup>
        <frame id="windowActions"
               screen="editWindowActions"/>
    </layout>
</window>
